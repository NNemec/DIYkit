# Transparent socks5 proxy - allows arbitrary programs to use socks5

        NAME=tsocks
     VERSION=1.8beta5
    DISTFILE=${NAME}-${VERSION}.tar.gz
 MASTER_SITE=http://download.sourceforge.net/project/tsocks/tsocks/1.8%20beta%205
 ARCHIVE_URL=${MASTER_SITE}/${DISTFILE}
   AVAILABLE="tsocks -sh"

. $NNlab_path/NNlab-include.sh

DOWNLOAD_ARCHIVE
UNPACK_ARCHIVE

cd $SRCDIR/$NAME
./configure --prefix $PREFIX
make
make install
