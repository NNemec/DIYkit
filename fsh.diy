# Fast SSH remote command execution

        NAME=fsh
     VERSION=1.2
    DISTFILE=${NAME}-${VERSION}.tar.gz
 MASTER_SITE=http://www.lysator.liu.se/${NAME}
 ARCHIVE_URL=${MASTER_SITE}/${DISTFILE}
   AVAILABLE="fsh -V"

. $DIYkit/DIYkit-include.sh

DOWNLOAD_ARCHIVE
UNPACK_ARCHIVE

cd $SRCDIR/$NAME
./configure --prefix $PREFIX
make
make install
