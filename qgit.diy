# Qt GUI for the GIT version control

        NAME=qgit
     GIT_URL=git://git.kernel.org/pub/scm/qgit/qgit4.git
   AVAILABLE="pkg-config --exists template"

. $DIYkit/DIYkit-include.sh

DIYkit depend git
DIYkit depend qconf

GIT_CLONE

######### autoconf
cd $SRCDIR/$NAME
qconf
./configure --prefix $PREFIX
make
make install
